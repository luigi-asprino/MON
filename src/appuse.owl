<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
  <owl:Ontology rdf:about="https://w3id.org/MON/appuse.owl">
    <owl:imports rdf:resource="https://w3id.org/MON/activity.owl"/>
  </owl:Ontology>
  <owl:Class rdf:about="https://w3id.org/MON/appuse.owl#AppUse">
    <rdfs:label xml:lang="en">App Use</rdfs:label>
    <rdfs:comment xml:lang="en">The activity of using an app by a Person.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="https://w3id.org/MON/action.owl#App"/>
        <owl:onProperty rdf:resource="https://w3id.org/MON/action.owl#executesTask"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://w3id.org/MON/activity.owl#Activity"/>
  </owl:Class>
</rdf:RDF>
