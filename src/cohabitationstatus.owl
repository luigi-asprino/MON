    <!-- http://www.ontologydesignpatterns.org/ont/mario/cohabitationstatus.owl#Co-HabitationStatus -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/mario/cohabitationstatus.owl#Co-HabitationStatus">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/timeindexedsituation.owl#TimeIndexedSituation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/spatial.owl#hasPlace"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/spatial.owl#Residence"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/timeindexedsituation.owl#forPatient"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/healthrole.owl#Patient"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/timeindexedsituation.owl#forEntity"/>
                <owl:allValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/person.owl#Person"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A time indexed situation representing the co-habitation status for a patient at a certain time.</rdfs:comment>
        <rdfs:label xml:lang="en">Co-Habitation status</rdfs:label>
    </owl:Class>
    


