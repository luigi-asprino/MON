        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/ess.owl#hasCondition"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/ess.owl#Activity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/ess.owl#hasCondition"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/ess.owl#Incontinence"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/ess.owl#hasCondition"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/ess.owl#MentalState"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/ess.owl#hasCondition"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/ess.owl#MobilityInBed"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/ess.owl#hasCondition"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/ess.owl#PatientGeneralCondition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/ess.owl#hasPressureSoresRisk"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/ess.owl#PressureSoresRisk"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/cga.owl#score"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An action aiming at assessing the Exton-Smith Scale for a patient.</rdfs:comment>
        <rdfs:label xml:lang="en">Exton-Smith Scale (ESS) Assessment</rdfs:label>
    </owl:Class>
    


