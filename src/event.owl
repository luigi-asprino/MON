<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
  <owl:Ontology rdf:about="https://w3id.org/MON/event.owl">
    <owl:imports rdf:resource="https://w3id.org/MON/time.owl"/>
    <owl:imports rdf:resource="https://w3id.org/MON/spatial.owl"/>
  </owl:Ontology>
  <owl:Class rdf:about="https://w3id.org/MON/event.owl#Event">
    <rdfs:label xml:lang="en">Event</rdfs:label>
    <rdfs:comment xml:lang="en">Any physical, social, or mental process, event, or state.</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:about="https://w3id.org/MON/time.owl#TemporalEntity"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <rdf:Description rdf:about="https://w3id.org/MON/spatial.owl#SpatialThing">
        <owl:disjointWith rdf:resource="https://w3id.org/MON/time.owl#TemporalEntity"/>
      </rdf:Description>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="https://w3id.org/MON/time.owl#TemporalEntity"/>
        <owl:onProperty rdf:resource="https://w3id.org/MON/time.owl#atTime"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="https://w3id.org/MON/spatial.owl#SpatialThing"/>
        <owl:onProperty rdf:resource="https://w3id.org/MON/spatial.owl#hasPlace"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="https://w3id.org/MON/action.owl#Agent"/>
        <owl:onProperty rdf:resource="https://w3id.org/MON/action.owl#hasParticipant"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://w3id.org/MON/event.owl#hasEvent">
    <rdfs:label>has event</rdfs:label>
    <rdfs:range rdf:resource="https://w3id.org/MON/event.owl#Event"/>
  </owl:ObjectProperty>
  <rdf:Description rdf:about="https://w3id.org/MON/action.owl#Action">
    <rdfs:subClassOf rdf:resource="https://w3id.org/MON/event.owl#Event"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/MON/action.owl#Task">
    <owl:disjointWith rdf:resource="https://w3id.org/MON/event.owl#Event"/>
  </rdf:Description>
</rdf:RDF>
