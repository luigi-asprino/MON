        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/cirs.owl#hasRating"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/mna.owl#HealthStatusComparisonRating"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The action of assessing the health status of a patient in comparison of people of the same age.</rdfs:comment>
        <rdfs:label xml:lang="en">Health status comparison Assessment</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/mario/mna.owl#HealthStatusComparisonRating -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/mario/mna.owl#HealthStatusComparisonRating">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/mna.owl#MNATestsRating"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/cga.owl#score"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/generic.owl#description"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The rating of the MNA of the health status of a patient in comparison with other people of the same age, e.g. if a patient considers his health as good as the health of people of the same age, the score is 1.</rdfs:comment>
        <rdfs:label xml:lang="en">Health status comparison rating</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/mario/mna.owl#IndependentlyLivingAssessment -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/mario/mna.owl#IndependentlyLivingAssessment">
