            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/clinicalact.owl#hasMember"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/mna.owl#PressureSoresOrSkinUlcersAssessment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/clinicalact.owl#hasMember"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/mna.owl#PsychologicalStressAssessment"/>
