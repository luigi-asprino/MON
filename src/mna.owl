        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/cirs.owl#hasRating"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/mna.owl#IndependentlyLivingRating"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The action of assessing the living status of a patient.</rdfs:comment>
        <rdfs:label xml:lang="en">Independently living Assessment</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/mario/mna.owl#IndependentlyLivingRating -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/mario/mna.owl#IndependentlyLivingRating">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/mna.owl#MNATestsRating"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/cga.owl#score"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/generic.owl#description"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The rating of living status in the Mini Nutritional Assessment (MNA), e.g. if the patient lives independently (not in a nursing home or hospital) the score in the MNA is 0.</rdfs:comment>
        <rdfs:label xml:lang="en">Independently living Rating</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/mario/mna.owl#LossOfAppetiteAssessment -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/mario/mna.owl#LossOfAppetiteAssessment">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/mna.owl#DietaryAssessment"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/cirs.owl#hasRating"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/mna.owl#LossOfAppetiteRating"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The action of assessing the loss of appetite of a patient.</rdfs:comment>
        <rdfs:label xml:lang="en">Loss of appetite Assessment</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/mario/mna.owl#LossOfAppetiteRating -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/mario/mna.owl#LossOfAppetiteRating">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/mna.owl#MNATestsRating"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/cga.owl#score"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/generic.owl#description"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The rating in the Mini Nutritional Assessment of the loss of appetite of a patient, e.g. if the patient has suffered of severe loss of appetite in the last three the score is 0.</rdfs:comment>
        <rdfs:label xml:lang="en">Loss of appetite Rating</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/mario/mna.owl#MACAssessment -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/mario/mna.owl#MACAssessment">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/mna.owl#AnthropometricAssessment"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/cirs.owl#hasRating"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/mna.owl#MACRating"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/mna.owl#hasMAC"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/measurement.owl#QuantitativeValue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The action of assessing the Mid Arm Circumference (MAC) of a Patient.</rdfs:comment>
        <rdfs:label xml:lang="en">Mid Arm Circumference (MAC) Assessment</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/mario/mna.owl#MACRating -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/mario/mna.owl#MACRating">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/mna.owl#MNATestsRating"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/mna.owl#hasMAC"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/mario/measurement.owl#QuantitativeValue"/>
                            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/mario/time.owl#Interval"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/mario/cga.owl#score"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The rating of the Mid Arm Circumference (MAC) in the Mini Nutritional Assessment (MNA).</rdfs:comment>
        <rdfs:label xml:lang="en">Mid Arm Circumference (MAC) Rating</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/mario/mna.owl#MNARating -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/mario/mna.owl#MNARating">
