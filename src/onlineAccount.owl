<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
  <owl:Ontology rdf:about="https://w3id.org/MON/onlineAccount.owl">
    <owl:imports rdf:resource="https://w3id.org/MON/person.owl"/>
    <owl:imports rdf:resource="https://w3id.org/MON/service.owl"/>
  </owl:Ontology>
  <owl:Class rdf:about="https://w3id.org/MON/onlineAccount.owl#OnlineAccount">
    <rdfs:label xml:lang="en">Online account</rdfs:label>
    <rdfs:comment xml:lang="en">This class represents the provision of some form of online service by some party to some Person.</rdfs:comment>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://w3id.org/MON/onlineAccount.owl#hasOnlineAccount">
    <rdfs:label xml:lang="en">has online account</rdfs:label>
    <rdfs:range rdf:resource="https://w3id.org/MON/onlineAccount.owl#OnlineAccount"/>
    <rdfs:domain rdf:resource="https://w3id.org/MON/person.owl#Person"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="https://w3id.org/MON/onlineAccount.owl#accountId">
    <rdfs:label xml:lang="en">account identifier</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="https://w3id.org/MON/onlineAccount.owl#OnlineAccount"/>
  </owl:DatatypeProperty>
  <owl:FunctionalProperty rdf:about="https://w3id.org/MON/onlineAccount.owl#hasService">
    <rdfs:range rdf:resource="https://w3id.org/MON/service.owl#OnlineService"/>
    <rdfs:domain rdf:resource="https://w3id.org/MON/onlineAccount.owl#OnlineAccount"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
</rdf:RDF>
