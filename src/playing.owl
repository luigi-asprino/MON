<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
  <owl:Ontology rdf:about="https://w3id.org/MON/playing.owl">
    <owl:imports rdf:resource="https://w3id.org/MON/activity.owl"/>
  </owl:Ontology>
  <owl:Class rdf:about="https://w3id.org/MON/playing.owl#Playing">
    <rdfs:subClassOf>
      <rdf:Description rdf:about="https://w3id.org/MON/activity.owl#Activity">
        <owl:equivalentClass>
          <owl:Restriction>
            <owl:hasValue rdf:resource="https://w3id.org/MON/activity.owl#PlayingTask"/>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#executesTask"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:equivalentClass>
      </rdf:Description>
    </rdfs:subClassOf>
  </owl:Class>
</rdf:RDF>
