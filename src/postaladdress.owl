<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
  <owl:Ontology rdf:about="https://w3id.org/MON/postalAddress.owl">
    <owl:imports rdf:resource="https://w3id.org/MON/spatial.owl"/>
  </owl:Ontology>
  <owl:Class rdf:about="https://w3id.org/MON/postalAddress.owl#PostalAddress">
    <rdfs:label xml:lang="en">Postal address</rdfs:label>
    <rdfs:comment xml:lang="en">An "address representation" as conceptually defined by the INSPIRE Address Representation data type.</rdfs:comment>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://w3id.org/MON/postalAddress.owl#hasPostalAddress">
    <rdfs:range rdf:resource="https://w3id.org/MON/postalAddress.owl#PostalAddress"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
  </owl:ObjectProperty>
  <owl:FunctionalProperty rdf:about="https://w3id.org/MON/postalAddress.owl#fullAddress">
    <rdfs:label xml:lang="en">full address</rdfs:label>
    <rdfs:comment xml:lang="en">The complete address written as a string, with or without formatting.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:domain rdf:resource="https://w3id.org/MON/postalAddress.owl#PostalAddress"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="https://w3id.org/MON/postalAddress.owl#poBox">
    <rdfs:label xml:lang="en">post office box number</rdfs:label>
    <rdfs:comment xml:lang="en">The Post Office Box number.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:domain rdf:resource="https://w3id.org/MON/postalAddress.owl#PostalAddress"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="https://w3id.org/MON/postalAddress.owl#addressArea">
    <rdfs:label xml:lang="en">address area</rdfs:label>
    <rdfs:comment xml:lang="en">The name or names of a geographic area or locality that groups a number of addressable objects for addressing purposes, without being an administrative unit. This would typically be part of a city, a neighbourhood or village.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:domain rdf:resource="https://w3id.org/MON/postalAddress.owl#PostalAddress"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="https://w3id.org/MON/postalAddress.owl#postCode">
    <rdfs:label xml:lang="en">post code</rdfs:label>
    <rdfs:comment xml:lang="en">The post code (a.k.a postal code, zip code etc.).</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:domain rdf:resource="https://w3id.org/MON/postalAddress.owl#PostalAddress"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="https://w3id.org/MON/postalAddress.owl#postName">
    <rdfs:label xml:lang="en">post name</rdfs:label>
    <rdfs:comment xml:lang="en">The key postal division of the address, usually the city.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:domain rdf:resource="https://w3id.org/MON/postalAddress.owl#PostalAddress"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="https://w3id.org/MON/postalAddress.owl#adminUnitL2">
    <rdfs:label xml:lang="en">admin unit L2</rdfs:label>
    <rdfs:comment xml:lang="en">The region of the address, usually a county, state or other such area that typically encompasses several localities.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:domain rdf:resource="https://w3id.org/MON/postalAddress.owl#PostalAddress"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="https://w3id.org/MON/postalAddress.owl#adminUnitL1">
    <rdfs:label xml:lang="en">admin unit L1</rdfs:label>
    <rdfs:comment xml:lang="en">The uppermost administrative unit for the address, almost always a country.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:domain rdf:resource="https://w3id.org/MON/postalAddress.owl#PostalAddress"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="https://w3id.org/MON/postalAddress.owl#locatorName">
    <rdfs:label xml:lang="en">locator name</rdfs:label>
    <rdfs:comment xml:lang="en">Proper noun(s) applied to the real world entity identified by the locator. The locator name could be the name of the property or complex, of the building or part of the building, or it could be the name of a room inside a building.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:domain rdf:resource="https://w3id.org/MON/postalAddress.owl#PostalAddress"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
</rdf:RDF>
