        <rdfs:domain rdf:resource="https://w3id.org/MON/rasr.owl#Subsystem"/>
        <rdfs:range rdf:resource="https://w3id.org/MON/rasr.owl#Component"/>
        <rdfs:label xml:lang="en">includes</rdfs:label>
    </owl:ObjectProperty>
    


