    <owl:ObjectProperty rdf:about="https://w3id.org/MON/rasr.owl#provides">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/2010/10/Ontology1288790966584.owl#component_offers_interface"/>
