<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:j.0="https://w3id.org/MON/service.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
  <owl:Ontology rdf:about="https://w3id.org/MON/service.owl"/>
  <owl:Class rdf:about="https://w3id.org/MON/service.owl#OnlineService">
    <rdfs:label xml:lang="en">Online service</rdfs:label>
    <rdfs:comment xml:lang="en">A service available on the web.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/MON/service.owl#Service"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/MON/service.owl#Functionality">
    <rdfs:label xml:lang="en">Functionality</rdfs:label>
    <rdfs:comment xml:lang="en">The purpose that something is designed or expected to fulfil.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/MON/service.owl#Service">
    <rdfs:label xml:lang="en">Service</rdfs:label>
    <rdfs:comment xml:lang="en">This class represents services as intended in economics, i.e. an intagible commodity.</rdfs:comment>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://w3id.org/MON/service.owl#hasFunctionality">
    <rdfs:range rdf:resource="https://w3id.org/MON/service.owl#Functionality"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/MON/service.owl#usingService">
    <rdfs:range rdf:resource="https://w3id.org/MON/service.owl#Service"/>
  </owl:ObjectProperty>
  <j.0:Functionality rdf:about="https://w3id.org/MON/service.owl#VideoCall">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </j.0:Functionality>
  <j.0:Service rdf:about="https://w3id.org/MON/service.owl#TelephoneNetwork">
    <j.0:hasFunctionality>
      <j.0:Functionality rdf:about="https://w3id.org/MON/service.owl#VoiceCall">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
      </j.0:Functionality>
    </j.0:hasFunctionality>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </j.0:Service>
</rdf:RDF>
